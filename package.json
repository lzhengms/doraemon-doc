{
  "name": "kakashi-doc",
  "version": "1.0.4",
  "description": "一款文档生成器",
  "main": "./bin/kakashi-doc.js",
  "directories": {
    "example": "examples"
  },
  "bin": {
    "kakashi-doc": "./bin/kakashi-doc.js"
  },
  "scripts": {
    "rebuild": "npm run build && lessc ./static/less/style.less ./static/css/main.css && node ./bin/kakashi-doc.js",
    "prepublish": "npm run build",
    "start": "concurrently 'npm run dev' 'npm run doc'",
    "build": "rm -rf lib && ./node_modules/.bin/babel src --out-dir lib --ignore __tests__",
    "dev": "./node_modules/.bin/babel src -s --out-dir lib --watch --ignore __tests__",
    "doc": "node bin/atool-doc.js --port 8987",
    "lint": "eslint src",
    "eslint-fix": "eslint --fix src",
    "gh-pages": "npm run doc -- --build && gh-pages -d __site",
    "pub": "npm run build && npm publish && npm run gh-pages",
    "beta": "npm run build && npm publish --tag beta && npm run gh-pages",
    "test": "npm run lint",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v-kakashi/kakashi-doc.git"
  },
  "author": "lmm0591@qq.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/v-kakashi/kakashi-doc/issues"
  },
  "homepage": "https://github.com/v-kakashi/kakashi-doc#readme",
  "dependencies": {
    "atool-doc-js-loader": "~0.1.0",
    "atool-doc-md-loader": "~0.2.0",
    "atool-doc-util": "~0.1.0",
    "atool-monitor": "0.x",
    "babel-cli": "~6.6.5",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "~0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-0": "~6.5.0",
    "chokidar": "~1.5.1",
    "commander": "~2.9.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "dora": "~0.3.0",
    "dora-plugin-proxy": "~0.6.1",
    "ejs": "^2.4.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "glob": "~7.0.3",
    "html-webpack-plugin": "^2.9.0",
    "json-loader": "^0.5.4",
    "koa-webpack-dev-middleware": "~1.1.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "raw-loader": "~0.5.1",
    "style-loader": "~0.13.0",
    "url-loader": "^0.5.7",
    "vue": "^1.0.25",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^8.5.3",
    "vue-style-loader": "^1.0.0",
    "webpack": "~1.12.2",
    "webpack-merge": "^0.14.0"
  },
  "devDependencies": {
    "concurrently": "^2.1.0",
    "eslint": "^2.0.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-html": "^1.5.1",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "~4.3.0",
    "eslint-plugin-standard": "^1.3.2",
    "gh-pages": "~0.11.0",
    "isomorphic-fetch": "~2.2.1",
    "pre-commit": "~1.1.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "pre-commit": [
    "lint"
  ]
}
